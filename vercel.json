{
  "buildCommand": "npm run build --workspace=@qregalo/web",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm install && (npm install @rollup/rollup-linux-x64-gnu@^4.57.0 --workspace=@qregalo/web --save-dev || true)",
  "framework": "astro",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
