{
  "buildCommand": "npm run build --workspace=@qregalo/web",
  "outputDirectory": "apps/web/dist",
  "framework": "astro",
  "functions": {
    "apps/web/.vercel/output/functions/_render.mjs": {
      "runtime": "nodejs20.x"
    },
    "apps/web/.vercel/output/functions/**/*.mjs": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
